{
  "name": "electron-serialport",
  "version": "1.0.2",
  "description": "A minimal Electron application with node serialport",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . software-light-switch --platform=win32 --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:serialport/electron-serialport.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "serialport": "^10.3.0",
    "tableify": "^1.1.1"
  },
  "build": {
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    }
  }
}
